    "@types/react-dom": "18.2.17",
    "autoprefixer": "^10.4.13",
    "eslint-config-custom": "*",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "tailwind-config": "*",
    "tailwindcss": "3.3.5",
    "tsconfig": "*",
    "tsup": "^6.0.1",
    "tw-elements": "^1.0.0-beta1",
    "typescript": "^5.2.2"
